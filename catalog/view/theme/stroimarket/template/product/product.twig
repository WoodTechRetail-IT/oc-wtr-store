{{ header }}
<div id="product-product">
  <section class="catalog-product__block product-content">
    <div class="container">
      <div class="catalog-product__container">
        <div class="navigation-content">
          <ul class="breadcrumb">
            {% for i,breadcrumb in breadcrumbs %} 
            {% if (i+1<breadcrumbs|length) %}
            <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
            {% else %}
            <li>{{ breadcrumb.text }}</li>
            {% endif %} 
            {% endfor %}
          </ul>
        </div>
        <div id="product" class="product">
          {% if back_history %}
          <div class="back-history"><a href="" class="button-back" onclick="window.history.back(); return false;">{{ text_back }}</a></div>
          {% endif %}
          {{ content_top }}
          <h1 class="main-title">{{ heading_title }}</h1>
          <div id="sectioncontent">
            
          </div>
          <div class="row product-row-main-pr">
            <div class="col-xxl-9 col-xl-9 col-md-7">
              <div class="row">
                          <div class="col-xl-6">
                          {% if pmtemplate == "vhodnyjedveri" %}



                      
                                <div class="nakladki perednajanakl">
                                  <a href="{{ popup }}" title="{{ heading_title }}"><img src="{{ thumb }}" title="{{ heading_title }}" alt="{{ heading_title }}" class="img-responsive"  id="mainimagez" /></a>
                                </div>
                                <div class="nakladki zadnajanaklvhodn" id="zadnyjanakl">
                                  <a href="{{ popup }}" title="{{ heading_title }}"><img src="{{ thumb }}" title="{{ heading_title }}" alt="{{ heading_title }}" class="img-responsive"  id="mainimagezfdd" /></a>
                                </div>
                              {% else %}



                                <div class="product-left">
                                  <div class="product-left-block">
                                    <div class="product-image">
                                      {% if thumb or images %}
                                      <div class="product-images">
                                        <div class="product-image__thumb">
                                          {% if images %}
                                          {% for image in images %}
                                          <div class="product-images__additional"><img src="{{ image.thumb }}" title="{{ heading_title }}" alt="{{ heading_title }}" class="img-responsive" /></div>
                                          {% endfor %}
                                          {% endif %}
                                        </div>
                                      </div>
                                      <div class="product-image__mains">
                                                {% if thumb %}
                                                    <div class="product-images__thumb">
                                                      <a href="{{ popup }}" title="{{ heading_title }}"><img src="{{ thumb }}" title="{{ heading_title }}" alt="{{ heading_title }}" class="img-responsive"  id="mainimagez" /></a>
                                                    </div>
                                                  {% endif %}
                                        
                                      </div>
                                      {% endif %}
                                    </div>
                                    
                                  </div>
                                </div>
                            {% endif %}
                            
                            {% if pmtemplate == "productdver" %}
                              <a href="#view-in-interior" class="phone-link phone-link-callback popup-with-move-anim view-interior-a open-popup-link"><div class="view-interior"><img src="/image/catalog/woodtech_item/frontend/interior/view-interior.png" class="view-interior-img"></div></a>
                            {% endif %}
                          </div>
                <div class="col-xl-6">
                        {% if hpmwtrdata %}
                        <div class="hpmwtrblock">
                          {% for hpmwtritem in hpmwtrdata %}
                          <div class="hpmwtrblock-item">
                            <div class="hpmwtrblock-item-blpict">
                              <img src="{{ hpmwtritem.image }}" mainimg="{{ hpmwtritem.image_main }}" idhhpm="{{ hpmwtritem.product_id }}" alt="{{ hpmwtritem.model }}" class="hpmwtrblock-item-blpict-img">
                            </div>
                            <div class="hpmwtrblock-item-price">{{ hpmwtritem.price }} руб.</div>
                          </div>
                          {% endfor %}
                        </div>
                        {% endif %}
                  {% if options %}
                  <div class="product-options">
                    {% for option in options %}
                    {% if option.type == 'select' %}
                    <div class="form-group{% if option.required %} required {% endif %}">
                      <label class="control-label" for="input-option{{ option.product_option_id }}">{{ option.name }}</label>
                      <select name="option[{{ option.product_option_id }}]" id="input-option{{ option.product_option_id }}" class="form-control form-control-select-wtr">
                        <option value="">{{ text_select }}</option>
                        {% for option_value in option.product_option_value %}
                        <option value="{{ option_value.product_option_value_id }}">{{ option_value.name }}
                          {% if option_value.price %}
                          ({{ option_value.price_prefix }}{{ option_value.price }})
                          {% endif %} 
                        </option>
                        {% endfor %}
                      </select>
                    </div>
                    {% endif %}
                    {% if option.type == 'radio' %}
                    <div class="radio-but-product form-group{% if option.required %} required {% endif %}">
                      <label class="control-label">{{ option.name }}</label>
                      <div id="input-option" class="input-option{{ option.option_id }} form-control-radio-wtr">
                        {% for option_value in option.product_option_value %}
                        

                        <div class="radio fake-radio">                
                          <input type="radio" id="{{ option_value.product_option_value_id }}" name="option[{{ option.product_option_id }}]" value="{{ option_value.product_option_value_id }}" />
                          <label class="option-btn radio-btn {% if option_value.imageb %}vhodntrue{% endif %}" data-image-zoomchange="{{ option_value.imageprod }}" {% if option_value.imageb %}data-vhodnimg="{{ option_value.imageb }}"{% endif %} data-toggle="tooltip" for="{{ option_value.product_option_value_id }}">
                            {% if not option_value.image %}
                            <span class="option-name" data-toggle="tooltip" title="{% if option_value.price %} {{ option_value.price_prefix }} {{ option_value.price }} {% endif %}">{{ option_value.name }}</span>
                            {% else %}
                            <span class="option-image" data-toggle="tooltip" title="{{ option_value.name }} {% if option_value.price %} {{ option_value.price_prefix }} {{ option_value.price }} {% endif %}">
                              <img src="{{ option_value.image }}" alt="{{ option_value.name }}" class="img-thumbnail" />
                            </span>
                            {% endif %} 
                          </label>
                        </div>
                        {% endfor %}
                      </div>
                    </div>
                    {% endif %}
                    {% if option.type == 'checkbox' %}
                    <div class="form-group{% if option.required %} required {% endif %}">
                      <label class="control-label">{{ option.name }}</label>
                      <div id="input-option" class="input-option{{ option.option_id }} option-grid form-control-wtr-checkbox">
                        {% for option_value in option.product_option_value %}
                        <div class="radio fake-checkbox">                
                          <input type="checkbox" id="{{ option_value.product_option_value_id }}" name="option[{{ option.product_option_id }}][]" value="{{ option_value.product_option_value_id }}" />
                          <label class="option-btn checkbox-btn" for="{{ option_value.product_option_value_id }}">
                            {% if not option_value.image %}
                            <span class="option-name" data-toggle="tooltip" title="{% if option_value.price %} {{ option_value.price_prefix }} {{ option_value.price }} {% endif %}">{{ option_value.name }}</span>
                            {% else %}
                            <span class="option-image" data-toggle="tooltip" title="{{ option_value.name }} {% if option_value.price %} {{ option_value.price_prefix }} {{ option_value.price }} {% endif %}">
                              <img src="{{ option_value.image }}" alt="{{ option_value.name }}" class="img-thumbnail" />
                            </span>
                            {% endif %} 
                          </label>
                        </div>
                        {% endfor %}
                      </div>
                    </div>
                    {% endif %}


                    {% endfor %}
                  </div>
                  {% endif %}

                  <div class="list-attributes">
                    <div class="list-attributes__title">{{ text_about_product }}</div>
                    <ul class="list-informations">
                      
                      {% if attributes %}
                      {% for attribute in attributes|slice(0, 5) %}
                      <li class="list-li">
                        <div class="list-col list-li__left">
                          <span class="li-name">{{ attribute.name }}:</span>
                        </div>
                        <div class="list-col list-li__right">{{ attribute.text }}</div>
                      </li>
                      {% endfor %}
                      <li class="all-tech"><a href="" onclick="$('a[href=\'#tab-specification\']').trigger('click');  $('html, body').animate({ scrollTop: $('a[href=\'#tab-specification\']').offset().top - 100}, 500); return false;">{{ text_all_attributes }}</a></li>
                      {% endif %}
                    </ul>
                  </div>
                  


                  {% if pmtemplate == "productpol" %}
                            {% if manufacturers_img %}
                            <div class="product-twobl product-block-price">
                                <div class="manufacturer-logo">
                                  <div class="manufacturer-logo__img">
                                    <img src="{{ manufacturers_img }}" alt="{{ manufacturer }}" title="{{ manufacturer }}" class="img-thumbnail" />
                                  </div>
                                  <a href="{{ manufacturers }}">Все товары {{ manufacturer }}</a>
                                </div>
                            </div>
                          {% else %}
                            <div class="product-twobl product-block-price">
                                  {% if manufacturer %}
                                      <div class="block-no-img-man">
                                        <div class="list-col list-li__left">
                                          <span class="li-name">{{ text_manufacturer }}</span>
                                        </div>
                                        <div class="list-col list-li__right">
                                          <a href="{{ manufacturers }}">{{ manufacturer }}</a>
                                        </div>
                                      </div>
                                  {% endif %}
                            </div>
                          {% endif %}
                          

                          {% if quantity == 0 %}
                          <div class="product-offers product-stock not-available">
                            <span><i class="fi fi-rr-cross-circle mr-2"></i>{{ stock }}</span>
                          </div>
                          {% else %}
                          <div class="product-offers product-stock in-stock">
                            <span><i class="fi fi-rr-badge-check mr-2"></i>{{ text_instock }} {% if stock_display %}{{ stock }} {{ text_pcs }}{% endif %}</span>
                          </div>
                          {% endif %}


                          <div class="product-tools form-group">
                          <div class="btn-addtools d-flex flex-wrap">
                            <button type="button" class="product-compare tools-toggle clear-button" onclick="compare.add('{{ product_id }}');"><i class="fi fi-rr-balance-scale-right"></i> <span>{{ button_compare }}</span></button>
                            <button type="button" class="product-wishlist tools-toggle clear-button" onclick="wishlist.add('{{ product_id }}');"><i class="fi fi-rr-heart"></i> <span>{{ button_wishlist }}</span></button>
                            {% if product_share %}
                            <div class="product-share">
                              <span class="dropdown-toggle" data-toggle="dropdown"><i class="fi fi-rr-share"></i></span>
                              <div class="product-share__block dropdown-menu dropdown-menu-right">
          
                              </div>
                            </div>
                            {% endif %}
                          </div>
                        </div>

                    {% endif %}

                </div>
              </div>
            </div>
            

            
            {% if pmtemplate == "productpol" %}
            <div class="col-xxl-3 col-xl-3 col-md-5">
              <div class="product-right">
                <div class="product-right-block">
                      <input type="text" name="quantity" value="{{ minimum }}" data-min="{{ minimum }}" data-max="{{ quantity }}" class="counter-input-hidden" />
                        <div class="product-counter-t">
                          <div class="counter-pol-bloc">
                            <div class="countblock-item countblock-item-control countblock-item-control-minus"><i class="fa fa-minus" aria-hidden="true"></i></div>
                            <div class="countblock-item countblock-item-inpt"><input onkeypress='validate(event)' type="text" value="{{ minimum }}" data-min="{{ minimum }}" class="counter-input-w" id="mainquantmtwo"/></div>
                            <div class="countblock-item countblock-item-control countblock-item-control-plus"><i class="fa fa-plus" aria-hidden="true"></i></div>
