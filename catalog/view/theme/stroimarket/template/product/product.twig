                            <div class="countblock-item countblock-item-control countblock-item-control-minus"><i class="fa fa-minus" aria-hidden="true"></i></div>
                            <div class="countblock-item countblock-item-inpt"><input onkeypress='validate(event)' type="text" value="1" class="counter-input-w" id="mainquanupac"/></div>
                            <div class="countblock-item countblock-item-control countblock-item-control-plus"><i class="fa fa-plus" aria-hidden="true"></i></div>
                          </div>
                        </div>
                        <div class="blockinfopricecalcfloor">
                          {% if pricem2 %}
                            {% if not specialm2 %}
                              <div class="blockrashetfloor" id="datafloor" classicprice="{{ pricem2 }}"></div>
                              {% else %}
                            <div class="blockrashetfloor" id="datafloor" classicprice="{{ pricem2 }}" specialprice="{{ specialm2 }}"></div>
                            {% endif %}
                          {% endif %}
                        </div>
                  <div class="product-buy add-cart-block">
                    <div class="product-buy__btn">
